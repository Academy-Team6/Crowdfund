@model ProjectModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>My Dashboard</h1>
To add a new project press <a href="~/home/addproject">here</a>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Title</th>
            <th scope="col">Description</th>
            <th scope="col">Budget Ratio</th>
            <th scope="col">Update</th>

        </tr>
    </thead>
    <tbody>
        @{
            foreach (var p in Model.Projects)
            {
                <tr>
                    <td>@p.Title</td>
                    <td>@p.Description</td>
                    <td>@p.BudgetRatio</td>
                    <td> <a href="~/home/UpdateProject/@p.Id"><img src="~/images/update.png" width="25" /></a></td>
                </tr>

            }
        }
    </tbody>
</table>
