@{ }
<div class="container-fluid">
    
        <div class="col-md-6 col-sm-12">
            <h1> Delete Backer</h1>

            <form>
                <table align="center">

                    <tr align="center">
                        <td>
                            Backer Id
                        </td>
                        <td> <input type="text" id="Id" /></td>
                    </tr>
                    <tr align="center">
                        <td>
                            <input type="button" value="Submit data" onclick="deleteBacker()" />
                        </td>
                    </tr>
                </table>
            </form>
        </div>
        <div class="col-md-6 col-sm-12">
            <h1> Find Backer</h1>

            <form>
                <table align="center">

                    <tr align="center">
                        <td>
                            Backer name
                        </td>
                        <td> <input type="text" id="searchText" /></td>
                    </tr>

                    <tr align="center">
                        <td>
                            <input type="button" value="Submit data" onclick="findBacker()" />
                        </td>
                    </tr>
                </table>
            </form>
        </div>
</div>
    
    

    

    
