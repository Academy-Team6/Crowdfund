@model ProjectModel

<meta charset="utf-8" />


<div class="container">
    <table id="Project Trending" border="1" cellpadding="10" cellspacing="0" class="table table-hover table-borderless table-sm table-info">
        <thead class="thead-light">
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Category</th>
                <th>Description</th>
                <th>TargetBudget</th>
                <th>CurrentBudget</th>
            </tr>
        </thead> 
        <tbody>
            @{
                foreach (var project in Model.Projects)
                {
                    <tr>
                        <td>@project.Id</td>
                        <td>@project.Title</td>
                        <td>@project.Category</td>
                        <td>@project.Description</td>
                        <td>@project.TargetBudget</td>
                        <td>@project.CurrentBudget</td>
                    </tr>
                }
            } 
        </tbody>
    </table>
</div>